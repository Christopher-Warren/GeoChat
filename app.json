      "bundleIdentifier": "com.almith.anonychat"
    },
    "android": {
      "versionCode": 12,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.almith.anonychat"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "e3d29f8d-2d2c-4c0c-ad3e-66b202cbc028"
      }
    }
  }
